{
  "name": "my_apps",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.6.0",
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^11.8.1",
    "dotenv-flow": "^4.1.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.27.5",
    "multer": "1.4.5-lts.1",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "pdf-lib": "^1.17.1",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "react-router-dom": "^7.1.3",
    "react-scripts": "5.0.1",
    "react-select": "^5.10.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "server": "nodemon -r dotenv/config ./server/index.js",
    "dev": "npm-run-all --parallel front-dev back-dev",
    "front-dev": "react-scripts start",
    "back-dev": "nodemon --watch src/backend --ext ts --exec 'ts-node src/backend/server.ts'",
    "start": "npm-run-all --parallel front-start back-start",
    "front-start": "react-scripts start",
    "back-start": "node ./build/backend/server.js",
    "build": "npm-run-all --parallel font-build back-build",
    "font-build": "react-scripts build",
    "back-build": "tsc",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "proxy": "http://localhost:8080"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.12",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.7",
    "@types/node-cron": "^3.0.11",
    "ajv": "^8.17.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "node-cron": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}